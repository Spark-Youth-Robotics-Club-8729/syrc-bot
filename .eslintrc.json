{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-var": "error",
        "eqeqeq": "error",
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
        "no-constant-condition":"off"
    }
}
